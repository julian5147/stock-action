# Server execution port
PORT=8000

# CockroachDB database configuration
DB_USER=root
DB_NAME=stocksdb
DB_PORT=26257
DATABASE_URL=postgresql://${DB_USER}@crdb:${DB_PORT}/${DB_NAME}?sslmode=disable

# Application environment (development/production)
APP_ENV=development

# External API URL for stock data
EXTERNAL_API_URL=https://api.example.com/stocks

# Authentication token for the external API (replace with your own token)
AUTH_TOKEN=your_auth_token_here

# Allowed origin for CORS
ALLOWED_ORIGIN=http://localhost:5173 